{
  "name": "lyon-text2",
  "version": "1.0.0",
  "description": "A text messaging app atop google apps script and google sheets",
  "keywords": [
    "react",
    "google",
    "apps",
    "script",
    "sheets",
    "text",
    "sms",
    "lyon",
    "text"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/enuchi/React-Google-Apps-Script.git"
  },
  "license": "MIT",
  "author": "Robert Quinn",
  "scripts": {
    "serve": "run-s webpack:serve",
    "start": "run-s push:dev webpack:serve",
    "test": "jest  --forceExit --detectOpenHandles --updateSnapshot",
    "test:integration": "jest --forceExit --detectOpenHandles --updateSnapshot test/local-development.test",
    "test:integration:extended": "cross-env IS_EXTENDED=true jest --forceExit --detectOpenHandles --updateSnapshot test/local-development.test",
    "certs:setup": "run-s mkdirp:certs certs:generate",
    "certs:generate": "mkcert -key-file ./certs/key.pem -cert-file ./certs/cert.pem localhost 127.0.0.1",
    "mkdirp:certs": "mkdirp certs",
    "mkdirp:dist": "mkdirp dist",
    "mv:claspjson": "mv ./dist/.clasp.json ./.clasp.json",
    "clasp:project:setup": "run-s rimraf:claspjson mkdirp:dist clasp:project:create mv:claspjson rimraf:dist",
    "clasp:project:create": "clasp create --type sheets --title \"My React Project\" --rootDir ./dist",
    "clasp:login": "clasp login",
    "clasp:open:addon": "clasp open --addon",
    "push:prod": "run-s build:prod shell:clasp:push:prod shell:clasp:reset",
    "push:dev": "run-s build:dev shell:clasp:push:dev shell:clasp:reset",
    "deploy:dev": "run-s shell:clasp:deploy:dev shell:clasp:reset",
    "deploy:prod": "run-s shell:clasp:deploy:prod shell:clasp:reset",
    "open:dev": "run-s shell:clasp:open:dev shell:clasp:reset",
    "open:prod": "run-s shell:clasp:open:prod shell:clasp:reset",
    "rimraf:dist": "rimraf dist",
    "rimraf:claspjson": "rimraf .clasp.json",
    "build:dev": "run-s rimraf:dist webpack:build:dev",
    "build:prod": "run-s rimraf:dist webpack:build:prod",
    "webpack:serve": "cross-env NODE_OPTIONS=--max_old_space_size=16384 NODE_ENV=development webpack serve",
    "webpack:build:dev": "cross-env NODE_OPTIONS=--max_old_space_size=16384 NODE_ENV=development webpack",
    "webpack:build:prod": "cross-env NODE_OPTIONS=--max_old_space_size=16384 NODE_ENV=production webpack",
    "shell:clasp:push:dev": "dotenv -- sh ./scripts/shell/clasp/push/dev.sh",
    "shell:clasp:push:prod": "dotenv -- sh ./scripts/shell/clasp/push/prod.sh",
    "shell:clasp:deploy:dev": "dotenv -- sh ./scripts/shell/clasp/deploy/dev.sh",
    "shell:clasp:deploy:prod": "dotenv -- sh ./scripts/shell/clasp/deploy/prod.sh",
    "shell:clasp:open:dev": "dotenv -- sh ./scripts/shell/clasp/open/dev.sh",
    "shell:clasp:open:prod": "dotenv -- sh ./scripts/shell/clasp/open/prod.sh",
    "shell:twilio:send:dev": "dotenv -- sh ./scripts/shell/twilio/send/dev.sh",
    "shell:twilio:send:prod": "dotenv -- sh ./scripts/shell/twilio/send/prod.sh",
    "shell:twilio:test:success": "dotenv -- sh ./scripts/shell/twilio/test/success.sh",
    "shell:twilio:test:failure": "dotenv -- sh ./scripts/shell/twilio/test/failure.sh",
    "shell:clasp:reset": "dotenv -- sh ./scripts/shell/clasp/reset.sh",
    "shell:env:example": "dotenv -- sh ./scripts/shell/env/example.sh",
    "sort": "sort-package-json",
    "node:debug": "",
    "husky:install": "husky install",
    "lint-staged": "lint-staged",
    "eslint:fix": "eslint --fix --ext .ts,.tsx .",
    "prettier:write": "prettier --write ./src/**/*.{ts,tsx,js,jsx}"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/lab": "^5.0.0-alpha.124",
    "@mui/material": "^5.11.16",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/google-apps-script": "^1.0.59",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-phone-input-2": "^2.15.1",
    "react-redux": "^8.0.5",
    "twilio": "^4.9.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@effortlessmotion/dynamic-cdn-webpack-plugin": "^5.0.1",
    "@effortlessmotion/html-webpack-inline-source-plugin": "^1.0.3",
    "@google/clasp": "^2.4.2",
    "@jest/globals": "^29.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/expect-puppeteer": "^5.0.3",
    "@types/jest": "^29.5.0",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "@types/jest-image-snapshot": "^6.1.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.10",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-googleappsscript": "^1.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gas-lib": "^2.0.4",
    "gas-types-detailed": "^1.1.1",
    "gas-webpack-plugin": "^2.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "jest-image-snapshot": "^6.1.0",
    "lint-staged": "^13.2.1",
    "mkdirp": "^2.1.6",
    "module-to-cdn": "^3.1.5",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.1.0",
    "postcss-preset-env": "^8.1.0",
    "prettier": "^2.8.7",
    "puppeteer": "^19.8.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react-refresh": "^0.14.0",
    "rimraf": "^4.4.1",
    "sort-package-json": "^2.4.1",
    "style-loader": "^3.3.2",
    "tailwindcss": "^3.3.1",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1"
  },
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=9.6.5"
  }
}
